
<div id="quarto-search-results"></div>

<% if (nav.navbar || nav.sidebar) { %>  

<header id="quarto-header" class="headroom fixed-top">

<% if (nav.navbar) { %>  

<nav class="navbar navbar-expand<%- nav.navbar["collapse-below"] %> navbar-<%- nav.navbar.type %> bg-<%- nav.navbar.background %> <%- nav.navbar.background === "none" || nav.navbar.background === "body" ? "border-bottom" : "" %>">
  <div class="container-fluid">
  <% partial('navbrand.ejs', nav.navbar) %>
  <% if (nav.navbar.left || nav.navbar.right) { %>  
    <% if (nav.navbar.collapse) { %>
      <% partial('navtoggle.ejs') %>
      <% partial('navcollapse.ejs') %>
        <% partial('navitems.ejs', { align: 'me-auto', items: nav.navbar.left}) %>
        <% partial('navitems.ejs', { align: '', items: nav.navbar.right }) %>
        <% if (nav.navbar.search) { %>
          <% partial('navsearch.ejs', { search: nav.navbar.search }) %>
        <% } %>
      </div> <!-- /navcollapse -->
    <% } else { %>
      <% partial('navitems.ejs', { align: 'me-auto', items: nav.navbar.left}) %>
      <% partial('navitems.ejs', { align: '', items: nav.navbar.right }) %>
      <% if (nav.navbar.search) { %>
        <% partial('navsearch.ejs', { search: nav.navbar.search }) %>
      <% } %>
    <% } %> 
  <% } else { %>
    <% if (nav.navbar.search) { %>
      <% partial('navsearch.ejs', { search: nav.navbar.search }) %>
    <% } %>
  <% } %>

  </div> <!-- /container-fluid -->
</nav>

<% } %>

<% if (nav.sidebar && nav.layout) { %>

<nav class="quarto-secondary-nav py-2 d-lg-none d-md-block bg-light" 
      data-bs-toggle="collapse" data-bs-target="#quarto-sidebar" 
      aria-controls="quarto-sidebar" aria-expanded="false" aria-label="Toggle sidebar navigation"
      onclick="if (window.quartoToggleHeadroom) { window.quartoToggleHeadroom(); }"
>
  <div class="container-fluid d-flex justify-content-between">
    <h1 class="quarto-secondary-nav-title mb-0 fs-5 d-inline"></h1>
    <button type="button" 
      class="quarto-btn-toggle btn bg-light d-lg-none py-0 px-1 btn-lg d-inline-block border-0">
      <i class="bi bi-chevron-down"></i>
    </button>
  </div>
</nav>

<% } %>

</header>

<% } %> <!-- /navbar/sidebar -->

<% if (nav.layout) { %>

<%
targetDiv = !nav.sidebar || nav.sidebar.style === "floating"
%>

<div class="container-fluid" <%= targetDiv ? 'id="quarto-content"' : "" %>>
<div class="row" <%= !targetDiv ? 'id="quarto-content"' : "" %>>

<% marginCss = "mx-auto" %>
<% if (nav.sidebar) { %>
  <% borderColor = nav.sidebar['border-color'] ? "border-" + nav.sidebar['border-color'] : "" %>
  <% marginCss = nav.sidebar.style === "floating" ? "mx-auto" : "me-auto" %>

  <div id="quarto-sidebar" class="col col-12 col-lg-3 d-lg-flex px-0 pt-0 sidebar collapse sidebar-navigation overflow-auto">
    <% partial('sidebar.ejs', { sidebar: nav.sidebar }) %>
  </div>
<% } %>

<div class="col mx-auto col-sm-12 col-md-9 col-lg-7 px-lg-4">
<main>

<% } %>




